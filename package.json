{
    "name": "@credenceanalytics/metabasecli",
    "version": "1.0.0",
    "description": "Credence Metabase CLI",
    "author": "vikasprabhu@credenceanalytics.com",
    "type": "commonjs",
    "bin": {
        "metabase": "./bin/run"
    },
    "homepage": "https://github.com/Credence-Analytics/metabase-cli",
    "license": "MIT",
    "main": "src/index.js",
    "repository": "https://github.com/Credence-Analytics/metabase-cli",
    "files": [
        "/bin",
        "/src",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "dependencies": {
        "@oclif/core": "^1.16.5",
        "@oclif/errors": "^1.3.4",
        "@oclif/plugin-help": "^5",
        "chalk": "^4.1.0",
        "cli-table3": "^0.6.0",
        "cli-ux": "^5.5.1",
        "dockerode": "^3.2.1",
        "enquirer": "^2.3.6",
        "fs-extra": "^9.0.1",
        "https": "^1.0.0",
        "listr": "^0.14.3",
        "listr-inquirer": "^0.1.0",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "monitorctrlc": "^2.0.1",
        "node-fetch": "^2.6.1",
        "update-notifier": "^5.0.1",
        "winston": "^3.3.3",
        "winston-daily-rotate-file": "^4.5.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@oclif/test": "^2.2.2",
        "adm-zip": "^0.4.16",
        "chai": "^4",
        "dir-compare": "^2.4.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-oclif": "^4",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^4.3.6",
        "lint-staged": "^10.5.3",
        "mocha": "^9",
        "mochawesome": "^6.2.1",
        "oclif": "^3",
        "prettier": "^2.7.1"
    },
    "oclif": {
        "bin": "metabase",
        "commands": "./src/commands",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "scripts": {
        "test": "mocha --forbid-only \"test/commands/metabase\""
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "bugs": "https://github.com/Credence-Analytics/metabase-cli/issues",
    "keywords": [
        "oclif",
        "metabase",
        "cli",
        "tool"
    ],
    "types": "src/index.js",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.{js,test.js}": [
            "prettier --write",
            "eslint"
        ]
    }
}
